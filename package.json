{
  "name": "anon_board",
  "version": "0.1.0",
  "description": "A fullstack MERN app for an anonymouse messagge board",
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "clientTest": "react-scripts test",
    "backend": "nodemon backend/server.js",
    "dev": "NODE_ENV=development concurrently --kill-others \"npm run backend\" \"npm run client\"",
    "server": "node backend/server.js",
    "production": "npm run build_frontend && NODE_ENV=production npm run server"
  },
  "keywords": [
    "fullstack",
    "mern"
  ],
  "author": "Martino Fenu",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
